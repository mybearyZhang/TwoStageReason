{
    "name": "testS-adapter-RCBH",
    "n_gpu": 1,
    "feature_model": {
        "type": "ResNet18Encoder",
        "args": {
            "num_channels": 3,
            "pretrained": false,
            "dropout": 0.13713746566589535,
            "freeze": true,
            "state": 2
        },
        "pretrained": {
            "path": "saved/best_models/MLP64/SVRT/model_best.pth",
            "cotrain": false
        }
    },
    "reason_model_disable": {
        "type": "Identity",
        "args": {}
    },
    "reason_model": {
        "type": "MLP",
        "args": {
            "input_size": 512,
            "hidden_sizes": [128],
            "output_size": 64,
            "freeze": true
        },
        "pretrained_dis": {
            "path": "saved/models/RCSBLH-shift2/1029_150821/checkpoint-epoch60.pth",
            "cotrain": true
        },
        "pretrained": {
            "path": "saved/best_models/MLP64/SVRT/model_best.pth",
            "cotrain": false
        }
    },
    "head_model": {
        "type": "MLPHead",
        "args": {
            "input_size": 64,
            "hidden_sizes": [8],
            "output_size": 2,
            "freeze": true
        },
        "pretrained": {
            "path": "saved/best_models/MLP64/SVRT/model_best.pth",
            "cotrain": false
        }
    },
    "data_loader": {
        "type": "SvrtDataLoader",
        "args":{
            "data_dir": "../DATA/svrt/results_problem_21/",
            "batch_size": 8,
            "shuffle": true,
            "split": "train",
            "validation_split": 0,
            "num_workers": 4
        }
    },
    "optimizer": {
        "type": "Adam",
        "args":{
            "lr": 7.422948632267287e-05,
            "weight_decay": 6.662823769423705e-05,
            "amsgrad": true
        }
    },
    "loss": "cross_entropy_loss",
    "metrics": [
        "accuracy"
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 50,
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 100,

        "save_dir": "saved",
        "save_period": 5,
        "verbosity": 2,
        
        "monitor": "min val_loss",
        "early_stop": 100,

        "adapter": {
            "input_size": 128,
            "output_size": 512,
            "lr": 1.422948632267287e-04,
            "weight_decay": 6.662823769423705e-05
        },

        "tensorboard": false,
        "wandb": false
    },
    "optuna_disable": {
        "n_trials": 100,
        "study_name": "testS_adpt_Res1_pretrained_RAVEN",
        "storage": "sqlite:///saved/optuna/testS_adpt.db",
        "load_if_exists": true,

        "models": {
            "dropout": {
                "min": 0,
                "max": 0.8
            }
        },
        "monitor":"max val_accuracy",
        "params": {
            "learning_rate": {
                "min": 1e-5,
                "max": 1e-2
            },
            "weight_decay": {
                "min": 1e-8,
                "max": 1e-3
            }
        }
    }
}
