{
    "name": "RCSBLH-shift1",
    "n_gpu": 4,
    "feature_models": {
        "RAVEN": {
            "type": "ResNet18Encoder",
            "args": {
                "num_channels": 16,
                "pretrained": false,
                "dropout": 0.3498992409645899,
                "state": 1,
                "freeze": true
            },
            "pretrained": {
                "path": "saved/best_models/MLP64/RAVEN/model_best.pth",
                "cotrain": false
            }
        },
        "CVR": {
            "type": "ResNet18Encoder",
            "args": {
                "num_channels": 3,
                "pretrained": false,
                "dropout": 0.4,
                "freeze": true,
                "state": 1,
                "reshape": "CVR"
            },
            "pretrained": {
                "path": "saved/best_models/MLP64/CVR/model_best.pth",
                "cotrain": false
            }
        },
        "SVRT": {
            "type": "ResNet18Encoder",
            "args": {
                "num_channels": 3,
                "pretrained": false,
                "dropout": 0.13713746566589535,
                "state": 1,
                "freeze": true
            },
            "pretrained": {
                "path": "saved/best_models/MLP64/SVRT/model_best.pth",
                "cotrain": false
            }
        },
        "Bongard": {
            "type": "ResNet18Encoder",
            "args": {
                "num_channels": 12,
                "pretrained": false,
                "dropout": 0.3498992409645899,
                "state": 1,
                "freeze": true
            },
            "pretrained": {
                "path": "saved/best_models/MLP64/Bongard/model_best.pth",
                "cotrain": false
            }
        },
        "BongardLogo": {
            "type": "ResNet18Encoder",
            "args": {
                "num_channels": 14,
                "pretrained": false,
                "dropout": 0.3498992409645899,
                "state": 1,
                "freeze": true
            },
            "pretrained": {
                "path": "saved/best_models/MLP64/BongardLogo/model_best.pth",
                "cotrain": false
            }
        },
        "BongardHOI": {
            "type": "ResNet18Encoder",
            "args": {
                "num_channels": 42,
                "pretrained": false,
                "dropout": 0.34733180040523426,
                "state": 1,
                "freeze": true
            },
            "pretrained": {
                "path": "saved/best_models/MLP64/BongardHOI/model_best.pth",
                "cotrain": false
            }
        }
    },
    "reason_model": {
        "type": "MLP",
        "args": {
            "input_size": 256,
            "hidden_sizes": [128],
            "output_size": 64
        }
    },
    "head_models": {
        "RAVEN": {
            "type": "MLPHead",
            "args": {
                "input_size": 64,
                "hidden_sizes": [8],
                "output_size": 8
            }
        },
        "CVR": {
            "type": "MLPHead",
            "args": {
                "input_size": 64,
                "hidden_sizes": [8],
                "output_size": 8,
                "reshape": "CVR"
            }
        },
        "SVRT": {
            "type": "MLPHead",
            "args": {
                "input_size": 64,
                "hidden_sizes": [8],
                "output_size": 2
            }
        },
        "Bongard": {
            "type": "MLPHead",
            "args": {
                "input_size": 64,
                "hidden_sizes": [8],
                "output_size": 2
            }
        },
        "BongardLogo": {
            "type": "MLPHead",
            "args": {
                "input_size": 64,
                "hidden_sizes": [8],
                "output_size": 2
            }
        },
        "BongardHOI": {
            "type": "MLPHead",
            "args": {
                "input_size": 64,
                "hidden_sizes": [8],
                "output_size": 2
            }
        }
    },
    "data_loaders": {
        "RAVEN": {
            "type": "RavenDataLoader",
            "args":{
                "data_dir": "../DATA/RAVEN-10000/",
                "batch_size": 16,
                "shuffle": true,
                "split": "train",
                "validation_split": 0,
                "num_workers": 4
            }
        },
        "CVR": {
            "type": "CvrDataLoader",
            "args":{
                "data_dir": "../DATA/CVR",
                "batch_size": 8,
                "shuffle": true,
                "split": "train",
                "validation_split": 0,
                "num_workers": 4,
                "n_samples": 1000
            }
        },
        "SVRT": {
            "type": "SvrtDataLoader",
            "args":{
                "data_dir": "../DATA/svrt/results_problem_21/",
                "batch_size": 8,
                "shuffle": true,
                "split": "train",
                "validation_split": 0,
                "num_workers": 4
            }
        },
        "Bongard": {
            "type": "BongardDataLoader",
            "args":{
                "data_dir": "../DATA/bongard/bongard-problems/",
                "batch_size": 16,
                "shuffle": true,
                "validation_split": 0.1,
                "num_workers": 4
            }
        },
        "BongardLogo": {
            "type": "BongardLogoDataLoader",
            "args":{
                "data_dir": "../DATA/bongard/ShapeBongard_V2",
                "batch_size": 16,
                "shuffle": true,
                "validation_split": 0.1,
                "num_workers": 4
            }
        },
        "BongardHOI": {
            "type": "BongardHoiDataLoader",
            "args":{
                "data_dir": "../DATA/hake",
                "batch_size": 16,
                "shuffle": true,
                "validation_split": 0.1,
                "num_workers": 4
            }
        } 
    },
    "optimizers": {
        "RAVEN": {
            "type": "Adam",
            "args":{
                "lr": 7.179440669483468e-05,
                "weight_decay": 1.0045281958616361e-08,
                "amsgrad": true
            }
        },
        "CVR": {
            "type": "Adam",
            "args":{
                "lr": 0.00029904578291150197,
                "weight_decay": 1.400912257368821e-07,
                "amsgrad": true
            }
        },
        "SVRT": {
            "type": "Adam",
            "args":{
                "lr": 7.422948632267287e-05,
                "weight_decay": 6.662823769423705e-05,
                "amsgrad": true
            }
        },
        "Bongard": {
            "type": "Adam",
            "args":{
                "lr": 5e-5,
                "weight_decay": 1e-7,
                "amsgrad": true
            }
        },
        "BongardLogo": {
            "type": "Adam",
            "args":{
                "lr": 3.1161582697548783e-05,
                "weight_decay": 1.5763202837786187e-05,
                "amsgrad": true
            }
        },
        "BongardHOI": {
            "type": "Adam",
            "args":{
                "lr": 0.00012109488400553044,
                "weight_decay": 1.8443672512857624e-05,
                "amsgrad": true
            }
        },
        "reasoning": {
            "type": "Adam",
            "args":{
                "lr": 5e-5,
                "weight_decay": 1e-7,
                "amsgrad": true
            }
        }
    },
    "lr_schedulers": {
        "RAVEN": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        },
        "CVR": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        },
        "SVRT": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        },
        "Bongard": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        },
        "BongardLogo": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        },
        "BongardHOI": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        },
        "reasoning": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        }
    },
    "losses": {
        "RAVEN": "cross_entropy_loss",
        "CVR": "cross_entropy_loss",
        "SVRT": "cross_entropy_loss",
        "Bongard": "cross_entropy_loss",
        "BongardLogo": "cross_entropy_loss",
        "BongardHOI": "cross_entropy_loss"
    },
    "metrics": {
        "RAVEN": ["accuracy"],
        "CVR": ["accuracy"],
        "SVRT": ["accuracy"],
        "Bongard": ["accuracy"],
        "BongardLogo": ["accuracy"],
        "BongardHOI": ["accuracy"]
    },
    "trainer": {
        "epochs": 100,

        "save_dir": "saved/",
        "save_period": 3,
        "verbosity": 2,
        
        "monitor": "max total_val_acc",
        "early_stop": 30,

        "entry_epochs": {
            "RAVEN": 0,
            "CVR": 0,
            "SVRT": 0,
            "Bongard": 0,
            "BongardLogo": 0,
            "BongardHOI": 0
        },
        "entry_epochs_2": {
            "RAVEN": 44,
            "CVR": 63,
            "SVRT": 0,
            "Bongard": 44,
            "BongardLogo": 6,
            "BongardHOI": 30
        },

        "pick_prob": true,

        "tensorboard": false,
        "wandb": true
    }
}
