{
    "name": "cotrain-Transformer",
    "n_gpu": 1,
    "feature_models": {
        "RAVEN": {
            "type": "ResNet18Encoder",
            "args": {
                "num_channels": 16,
                "pretrained": false,
                "dropout": 0.4196477347556195
            }
        },
        "CVR": {
            "type": "ResNet18Encoder",
            "args": {
                "num_channels": 3,
                "pretrained": false,
                "dropout": 0.15432838930840997,
                "reshape": "CVR"
            }
        },
        "SVRT": {
            "type": "ResNet18Encoder",
            "args": {
                "num_channels": 3,
                "pretrained": false,
                "dropout": 0.30463863775434025
            }
        },
        "Bongard": {
            "type": "ResNet18Encoder",
            "args": {
                "num_channels": 12,
                "pretrained": false,
                "dropout": 0.376406931423799
            }
        },
        "BongardLogo": {
            "type": "ResNet18Encoder",
            "args": {
                "num_channels": 14,
                "pretrained": false,
                "dropout": 0.27317671459682796
            }
        },
        "BongardHOI": {
            "type": "ResNet18Encoder",
            "args": {
                "num_channels": 42,
                "pretrained": false,
                "dropout": 0.7162850809166676
            }
        },
        "CophyBall": {
            "type": "CFEncoder",
            "args": {
                "state_size": 14,
                "z_size": 512
            }
        },
        "CophyTower": {
            "type": "CFEncoder",
            "args": {
                "state_size": 14,
                "z_size": 512
            }
        },
        "CophyColli": {
            "type": "CFEncoder",
            "args": {
                "state_size": 14,
                "z_size": 512
            }
        },
        "VQA": {
            "type": "VQAencoder",
            "args": {
                "embed_size": 512,
                "qst_vocab_size": 13189, 
                "word_embed_size": 300, 
                "qst_embed_size": 512, 
                "num_layers": 2, 
                "hidden_size": 512
            }
        }
    },
    "reason_model": {
        "type": "Transformer",
        "args": {
            "input_size": 512,
            "output_size": 64
        }
    },
    "head_models": {
        "RAVEN": {
            "type": "MLPHead",
            "args": {
                "input_size": 64,
                "hidden_sizes": [8],
                "output_size": 8
            }
        },
        "CVR": {
            "type": "MLPHead",
            "args": {
                "input_size": 64,
                "hidden_sizes": [8],
                "output_size": 8,
                "reshape": "CVR"
            }
        },
        "SVRT": {
            "type": "MLPHead",
            "args": {
                "input_size": 64,
                "hidden_sizes": [8],
                "output_size": 2
            }
        },
        "Bongard": {
            "type": "MLPHead",
            "args": {
                "input_size": 64,
                "hidden_sizes": [8],
                "output_size": 2
            }
        },
        "BongardLogo": {
            "type": "MLPHead",
            "args": {
                "input_size": 64,
                "hidden_sizes": [8],
                "output_size": 2
            }
        },
        "BongardHOI": {
            "type": "MLPHead",
            "args": {
                "input_size": 64,
                "hidden_sizes": [8],
                "output_size": 2
            }
        },
        "CophyBall": {
            "type": "CFDecoder",
            "args": {
                "state_size": 14,
                "z_size": 64
            }
        },
        "CophyTower": {
            "type": "CFDecoder",
            "args": {
                "state_size": 14,
                "z_size": 64
            }
        },
        "CophyColli": {
            "type": "CFDecoder",
            "args": {
                "state_size": 14,
                "z_size": 64
            }
        },
        "VQA": {
            "type": "MLPHead",
            "args": {
                "input_size": 64,
                "hidden_sizes": [64],
                "output_size": 1000
            }
        }
    },
    "data_loaders": {
        "RAVEN": {
            "type": "RavenDataLoader",
            "args": {
                "data_dir": "../DATA/RAVEN-10000/",
                "batch_size": 16,
                "shuffle": true,
                "split": "train",
                "validation_split": 0,
                "num_workers": 4
            }
        },
        "CVR": {
            "type": "CvrDataLoader",
            "args":{
                "data_dir": "../DATA/CVR",
                "batch_size": 8,
                "shuffle": true,
                "split": "train",
                "validation_split": 0,
                "num_workers": 4,
                "n_samples": 1000
            }
        },
        "SVRT": {
            "type": "SvrtDataLoader",
            "args":{
                "data_dir": "../DATA/svrt/results_problem_21/",
                "batch_size": 8,
                "shuffle": true,
                "split": "train",
                "validation_split": 0,
                "num_workers": 4
            }
        },
        "Bongard": {
            "type": "BongardDataLoader",
            "args":{
                "data_dir": "../DATA/bongard/bongard-problems/",
                "batch_size": 16,
                "shuffle": true,
                "validation_split": 0.1,
                "num_workers": 4
            }
        },
        "BongardLogo": {
            "type": "BongardLogoDataLoader",
            "args":{
                "data_dir": "../DATA/bongard/ShapeBongard_V2",
                "batch_size": 16,
                "shuffle": true,
                "validation_split": 0.1,
                "num_workers": 8
            }
        },
        "BongardHOI": {
            "type": "BongardHoiDataLoader",
            "args":{
                "data_dir": "../DATA/hake",
                "batch_size": 16,
                "shuffle": true,
                "validation_split": 0.1,
                "num_workers": 4
            }
        },
        "CophyBall": {
            "type": "FCBallsDataLoader",
            "args": {
                "data_dir": "../DATA/cophydataset/OUTBALLS",
                "batch_size": 16,
                "shuffle": true,
                "validation_split": 0.1,
                "num_workers": 6
            }
        },  
        "CophyTower": {
            "type": "FCBallsDataLoader",
            "args": {
                "data_dir": "../DATA/cophydataset/OUTPUT_PATH",
                "batch_size": 16,
                "shuffle": true,
                "validation_split": 0.1,
                "num_workers": 6
            }
        },  
        "CophyColli": {
            "type": "FCCollisionDataLoader",
            "args": {
                "data_dir": "../DATA/cophydataset/OUTCOLLISION",
                "batch_size": 16,
                "shuffle": true,
                "validation_split": 0.1,
                "num_workers": 6
            }
        },
        "VQA": {
            "type": "VQADataLoader",
            "args": {
                "data_dir": "../DATA/COCO-2015/Resized_Images",
                "batch_size": 16,
                "shuffle": true,
                "validation_split": 0.1,
                "num_workers": 6,
                "n_samples": 8000
            }
        }
    },
    "optimizers": {
        "RAVEN": {
            "type": "Adam",
            "args": {
                "lr": 2.6540844353989338e-05,
                "weight_decay": 0.00019618755153559414,
                "amsgrad": true
            }
        },
        "CVR": {
            "type": "Adam",
            "args":{
                "lr": 0.00021059684578837517,
                "weight_decay": 0.00012817068060662356,
                "amsgrad": true
            }
        },
        "SVRT": {
            "type": "Adam",
            "args":{
                "lr": 4.196996280247753e-05,
                "weight_decay": 4.3535441626887e-05,
                "amsgrad": true
            }
        },
        "Bongard": {
            "type": "Adam",
            "args":{
                "lr": 6.986423262662905e-05,
                "weight_decay": 3.0573179191733582e-06,
                "amsgrad": true
            }
        },
        "BongardLogo": {
            "type": "Adam",
            "args":{
                "lr": 1.007844877008617e-05,
                "weight_decay": 2.7863209325277617e-06,
                "amsgrad": true
            }
        },
        "BongardHOI": {
            "type": "Adam",
            "args":{
                "lr": 1.8496877371959032e-06,
                "weight_decay": 7.969496093822739e-06,
                "amsgrad": true
            }
        },
        "CophyBall": {
            "type": "Adam",
            "args":{
                "lr": 0.00043486354565642495,
                "weight_decay": 3.7815966683554253e-06,
                "amsgrad": true
            }
        },
        "CophyTower": {
            "type": "Adam",
            "args":{
                "lr": 0.001992414017909575,
                "weight_decay": 3.2305643561492355e-07,
                "amsgrad": true
            }
        },
        "CophyColli": {
            "type": "Adam",
            "args":{
                "lr": 0.0011009503037080074,
                "weight_decay": 2.798197487260918e-08,
                "amsgrad": true
            }
        },
        "VQA": {
            "type": "Adam",
            "args":{
                "lr": 0.006819735857126263,
                "weight_decay": 1.2618760078995198e-06,
                "amsgrad": true
            }
        },
        "reasoning": {
            "type": "Adam",
            "args":{
                "lr": 5e-5,
                "weight_decay": 1e-7,
                "amsgrad": true
            }
        }
    },
    "lr_schedulers": {
        "RAVEN": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        },
        "CVR": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        },
        "SVRT": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        },
        "Bongard": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        },
        "BongardLogo": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        },
        "BongardHOI": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        },
        "CophyBall": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        },
        "CophyTower": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        },
        "CophyColli": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        },
        "VQA": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        },
        "reasoning": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        }
    },
    "losses": {
        "RAVEN": "cross_entropy_loss",
        "CVR": "cross_entropy_loss",
        "SVRT": "cross_entropy_loss",
        "Bongard": "cross_entropy_loss",
        "BongardLogo": "cross_entropy_loss",
        "BongardHOI": "cross_entropy_loss",
        "CophyBall": "mse_loss",
        "CophyTower": "mse_loss",
        "CophyColli": "mse_loss",
        "VQA": "vqa_loss"
    },
    "metrics": {
        "RAVEN": ["accuracy"],
        "CVR": ["accuracy"],
        "SVRT": ["accuracy"],
        "Bongard": ["accuracy"],
        "BongardLogo": ["accuracy"],
        "BongardHOI": ["accuracy"],
        "CophyBall": ["CFScore"],
        "CophyTower": ["CFScore"],
        "CophyColli": ["CFScore"],
        "VQA": ["VQAaccuracy"]
    },
    "trainer": {
        "epochs": 100,

        "save_dir": "saved/",
        "save_period": 3,
        "verbosity": 2,
        
        "monitor": "max total_val_acc",
        "early_stop": 30,

        "entry_epochs": {
            "RAVEN": 42,
            "CVR": 63,
            "SVRT": 0,
            "Bongard": 42,
            "BongardLogo": 0,
            "BongardHOI": 30,
            "CophyBall": 0,
            "CophyTower": 0,
            "CophyColli": 0,
            "VQA": 0
        },

        "pick_prob": true,

        "tensorboard": false,
        "wandb": true
    }
}
