{
    "name": "cotrain-MLP",
    "n_gpu": 1,
    "feature_models": {
        "RAVEN": {
            "type": "ResNet18Encoder",
            "args": {
                "num_channels": 16,
                "pretrained": false,
                "dropout": 0.3498992409645899
            }
        },
        "CVR": {
            "type": "ResNet18Encoder",
            "args": {
                "num_channels": 3,
                "pretrained": false,
                "dropout": 0.4,
                "reshape": "CVR"
            }
        },
        "SVRT": {
            "type": "ResNet18Encoder",
            "args": {
                "num_channels": 3,
                "pretrained": false,
                "dropout": 0.13713746566589535
            }
        },
        "Bongard": {
            "type": "ResNet18Encoder",
            "args": {
                "num_channels": 12,
                "pretrained": false,
                "dropout": 0.3498992409645899
            }
        },
        "BongardLogo": {
            "type": "ResNet18Encoder",
            "args": {
                "num_channels": 14,
                "pretrained": false,
                "dropout": 0.3498992409645899
            }
        },
        "BongardHOI": {
            "type": "ResNet18Encoder",
            "args": {
                "num_channels": 42,
                "pretrained": false,
                "dropout": 0.34733180040523426
            }
        },
        "CophyBall": {
            "type": "CFEncoder",
            "args": {
                "state_size": 14,
                "z_size": 512
            }
        },
        "CophyTower": {
            "type": "CFEncoder",
            "args": {
                "state_size": 14,
                "z_size": 512
            }
        },
        "CophyColli": {
            "type": "CFEncoder",
            "args": {
                "state_size": 14,
                "z_size": 512
            }
        },
        "VQA": {
            "type": "VQAencoder",
            "args": {
                "embed_size": 512,
                "qst_vocab_size": 13189, 
                "word_embed_size": 300, 
                "qst_embed_size": 512, 
                "num_layers": 2, 
                "hidden_size": 512
            }
        }
    },
    "reason_model": {
        "type": "MLP",
        "args": {
            "input_size": 512,
            "hidden_sizes": [128],
            "output_size": 64
        }
    },
    "head_models": {
        "RAVEN": {
            "type": "MLPHead",
            "args": {
                "input_size": 64,
                "hidden_sizes": [8],
                "output_size": 8
            }
        },
        "CVR": {
            "type": "MLPHead",
            "args": {
                "input_size": 64,
                "hidden_sizes": [8],
                "output_size": 8,
                "reshape": "CVR"
            }
        },
        "SVRT": {
            "type": "MLPHead",
            "args": {
                "input_size": 64,
                "hidden_sizes": [8],
                "output_size": 2
            }
        },
        "Bongard": {
            "type": "MLPHead",
            "args": {
                "input_size": 64,
                "hidden_sizes": [8],
                "output_size": 2
            }
        },
        "BongardLogo": {
            "type": "MLPHead",
            "args": {
                "input_size": 64,
                "hidden_sizes": [8],
                "output_size": 2
            }
        },
        "BongardHOI": {
            "type": "MLPHead",
            "args": {
                "input_size": 64,
                "hidden_sizes": [8],
                "output_size": 2
            }
        },
        "CophyBall": {
            "type": "CFDecoder",
            "args": {
                "state_size": 14,
                "z_size": 64
            }
        },
        "CophyTower": {
            "type": "CFDecoder",
            "args": {
                "state_size": 14,
                "z_size": 64
            }
        },
        "CophyColli": {
            "type": "CFDecoder",
            "args": {
                "state_size": 14,
                "z_size": 64
            }
        },
        "VQA": {
            "type": "MLPHead",
            "args": {
                "input_size": 64,
                "hidden_sizes": [64],
                "output_size": 1000
            }
        }
    },
    "data_loaders": {
        "RAVEN": {
            "type": "RavenDataLoader",
            "args": {
                "data_dir": "../DATA/RAVEN-10000/",
                "batch_size": 16,
                "shuffle": true,
                "split": "train",
                "validation_split": 0,
                "num_workers": 4,
                "n_samples": -1
            }
        },
        "CVR": {
            "type": "CvrDataLoader",
            "args":{
                "data_dir": "../DATA/CVR",
                "batch_size": 8,
                "shuffle": true,
                "split": "train",
                "validation_split": 0,
                "num_workers": 4,
                "n_samples": 1000
            }
        },
        "SVRT": {
            "type": "SvrtDataLoader",
            "args":{
                "data_dir": "../DATA/svrt/results_problem_21/",
                "batch_size": 8,
                "shuffle": true,
                "split": "train",
                "validation_split": 0,
                "num_workers": 4,
                "n_samples": -1
            }
        },
        "Bongard": {
            "type": "BongardDataLoader",
            "args":{
                "data_dir": "../DATA/bongard/bongard-problems/",
                "batch_size": 16,
                "shuffle": true,
                "validation_split": 0.1,
                "num_workers": 4
            }
        },
        "BongardLogo": {
            "type": "BongardLogoDataLoader",
            "args":{
                "data_dir": "../DATA/bongard/ShapeBongard_V2",
                "batch_size": 16,
                "shuffle": true,
                "validation_split": 0.1,
                "num_workers": 8,
                "n_samples": -1
            }
        },
        "BongardHOI": {
            "type": "BongardHoiDataLoader",
            "args":{
                "data_dir": "../DATA/hake",
                "batch_size": 16,
                "shuffle": true,
                "validation_split": 0.1,
                "num_workers": 4,
                "n_samples": -1
            }
        },
        "CophyBall": {
            "type": "FCBallsDataLoader",
            "args": {
                "data_dir": "../DATA/cophydataset/OUTBALLS",
                "batch_size": 16,
                "shuffle": true,
                "validation_split": 0.1,
                "num_workers": 6,
                "n_samples": -1
            }
        },  
        "CophyTower": {
            "type": "FCBlockTowerDataLoader",
            "args": {
                "data_dir": "../DATA/cophydataset/OUTPUT_PATH",
                "batch_size": 16,
                "shuffle": true,
                "validation_split": 0.1,
                "num_workers": 6,
                "n_samples": -1
            }
        },  
        "CophyColli": {
            "type": "FCCollisionDataLoader",
            "args": {
                "data_dir": "../DATA/cophydataset/OUTCOLLISION",
                "batch_size": 16,
                "shuffle": true,
                "validation_split": 0.1,
                "num_workers": 6,
                "n_samples": -1
            }
        },
        "VQA": {
            "type": "VQADataLoader",
            "args": {
                "data_dir": "../DATA/COCO-2015/Resized_Images",
                "batch_size": 16,
                "shuffle": true,
                "validation_split": 0.1,
                "num_workers": 6,
                "n_samples": 8000
            }
        }
    },
    "optimizers": {
        "RAVEN": {
            "type": "Adam",
            "args": {
                "lr": 3.376068210892354e-05,
                "weight_decay": 8.563911391513883e-07,
                "amsgrad": true
            }
        },
        "CVR": {
            "type": "Adam",
            "args":{
                "lr": 0.0001809522698105296,
                "weight_decay": 2.693835860467779e-08,
                "amsgrad": true
            }
        },
        "SVRT": {
            "type": "Adam",
            "args":{
                "lr": 3.139079624268704e-05,
                "weight_decay": 3.5207840315970866e-08,
                "amsgrad": true
            }
        },
        "Bongard": {
            "type": "Adam",
            "args":{
                "lr": 5e-5,
                "weight_decay": 1e-7,
                "amsgrad": true
            }
        },
        "BongardLogo": {
            "type": "Adam",
            "args":{
                "lr": 1.159592371350494e-05,
                "weight_decay": 0.0001349729026160705,
                "amsgrad": true
            }
        },
        "BongardHOI": {
            "type": "Adam",
            "args":{
                "lr": 0.00012109488400553044?,
                "weight_decay": 1.8443672512857624e-05?,
                "amsgrad": true
            }
        },
        "CophyBall": {
            "type": "Adam",
            "args":{
                "lr": 1.0787191197249031e-05,
                "weight_decay": 0.00048139835623207727,
                "amsgrad": true
            }
        },
        "CophyTower": {
            "type": "Adam",
            "args":{
                "lr": 3.583906755620858e-05,
                "weight_decay": 2.4973045863077072e-05,
                "amsgrad": true
            }
        },
        "CophyColli": {
            "type": "Adam",
            "args":{
                "lr": 0.00283407107544701,
                "weight_decay": 3.332283718119061e-06,
                "amsgrad": true
            }
        },
        "VQA": {
            "type": "Adam",
            "args":{
                "lr": 0.005069567039065942,
                "weight_decay": 0.00019944711086909874,
                "amsgrad": true
            }
        },
        "reasoning": {
            "type": "Adam",
            "args":{
                "lr": 5e-5,
                "weight_decay": 1e-7,
                "amsgrad": true
            }
        }
    },
    "lr_schedulers": {
        "RAVEN": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        },
        "CVR": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        },
        "SVRT": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        },
        "Bongard": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        },
        "BongardLogo": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        },
        "BongardHOI": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        },
        "CophyBall": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        },
        "CophyTower": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        },
        "CophyColli": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        },
        "VQA": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        },
        "reasoning": {
            "type": "StepLR",
            "args": {
                "step_size": 50,
                "gamma": 0.1
            }
        }
    },
    "losses": {
        "RAVEN": "cross_entropy_loss",
        "CVR": "cross_entropy_loss",
        "SVRT": "cross_entropy_loss",
        "Bongard": "cross_entropy_loss",
        "BongardLogo": "cross_entropy_loss",
        "BongardHOI": "cross_entropy_loss",
        "CophyBall": "mse_loss",
        "CophyTower": "mse_loss",
        "CophyColli": "mse_loss",
        "VQA": "vqa_loss"
    },
    "metrics": {
        "RAVEN": ["accuracy"],
        "CVR": ["accuracy"],
        "SVRT": ["accuracy"],
        "Bongard": ["accuracy"],
        "BongardLogo": ["accuracy"],
        "BongardHOI": ["accuracy"],
        "CophyBall": ["CFScore"],
        "CophyTower": ["CFScore"],
        "CophyColli": ["CFScore"],
        "VQA": ["VQAaccuracy"]
    },
    "trainer": {
        "epochs": 100,

        "save_dir": "saved/",
        "save_period": 3,
        "verbosity": 2,
        
        "monitor": "max total_val_acc",
        "early_stop": 30,

        "entry_epochs": {
            "RAVEN": 42,
            "CVR": 63,
            "SVRT": 0,
            "Bongard": 42,
            "BongardLogo": 0,
            "BongardHOI": 30,
            "CophyBall": 0,
            "CophyTower": 0,
            "CophyColli": 0,
            "VQA": 0
        },

        "pick_prob": true,

        "tensorboard": false,
        "wandb": true
    }
}
