{
    "name": "R-C",
    "n_gpu": 1,
    "feature_model222": {
        "type": "ViTEncoder",
        "args": {
            "num_channels": 16,
            "pretrained": false,
            "image_size": 80
        }
    },
    "feature_model": {
        "type": "ResNet18Encoder",
        "args": {
            "num_channels": 16,
            "pretrained": false,
            "dropout": 0.3498992409645899,
            "state": -3
        }
    },
    "reason_model": {
        "type": "MLP",
        "args": {
            "input_size": 512,
            "hidden_sizes": [128],
            "output_size": 64,
            "freeze": false
        }
    },
    "head_model": {
        "type": "MLPHead",
        "args": {
            "input_size": 64,
            "hidden_sizes": [8],
            "output_size": 8
        }
    },
    "data_loader": {
        "type": "RavenDataLoader",
        "args":{
            "data_dir": "../DATA/RAVEN-10000/",
            "batch_size": 16,
            "shuffle": true,
            "split": "train",
            "validation_split": 0,
            "num_workers": 4
        }
    },
    "optimizer": {
        "type": "Adam",
        "args":{
            "lr": 7.179440669483468e-05,
            "weight_decay": 1.0045281958616361e-08,
            "amsgrad": true
        }
    },
    "loss": "cross_entropy_loss",
    "metrics": [
        "accuracy"
    ],
    "lr_scheduler": {
        "type": "StepLR",
        "args": {
            "step_size": 50,
            "gamma": 0.1
        }
    },
    "trainer": {
        "epochs": 100,

        "save_dir": "saved/",
        "save_period": 5,
        "verbosity": 2,
        
        "monitor": "min val_loss",
        "early_stop": 10,

        "tensorboard": false,
        "wandb": false
    },
    "optuna": {
        "n_trials": 100,
        "study_name": "RAVEN_ResNet18_7_fix",
        "storage": "sqlite:///saved/optuna/raven_shift.db",
        "load_if_exists": true,

        "models": {
            "dropout": {
                "min": 0,
                "max": 0.8
            }
        },
        "monitor":"max val_accuracy",
        "params": {
            "learning_rate": {
                "min": 1e-5,
                "max": 1e-2
            },
            "weight_decay": {
                "min": 1e-8,
                "max": 1e-3
            }
        }
    }
}
